{
	"name": "doxygen-previewer",
	"displayName": "Doxygen Previewer",
	"description": "Show the preview Doxygen documentation.",
	"publisher": "hakua",
	"author": "hakua-doublemoon",
	"license": "MIT",
	"readme": "README.md",
	"version": "0.2.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/hakua-doublemoon/DoxygenPreviewer"
	},
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.DoxygenPreviewer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.DoxygenPreviewer",
				"title": "Doxygen Previewer"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"dependencies": {
		"fs-extra": "^8.1.0"
	},
	"devDependencies": {
		"@types/fs-extra": "^8.0.0",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.14.13",
		"tslint": "^5.12.1",
		"typescript": "^3.5.3",
		"vscode": "^1.1.28"
	}
}
